{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "views/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/alldata", "dest": "/app.js" },
      { "src": "/viewdata", "dest": "/app.js" },
      { "src": "/universities/:countryName", "dest": "/app.js" },
      { "src": "/(.*)", "dest": "/public/index.html" }
    ]
  }
  