{
    "version": 2,
    "routes": [
      { "src": "/alldata", "dest": "/app.js" },
      { "src": "/viewdata", "dest": "/app.js" },
      { "src": "/universities/:countryName", "dest": "http://universities.hipolabs.com/search?country=:countryName" },
      { "src": "/(.+\\.html)", "dest": "$1" },
      { "src": "/(.+\\.json)", "dest": "$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  